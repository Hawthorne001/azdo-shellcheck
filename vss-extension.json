{
  "manifestVersion": 1,
  "id": "shellcheck",
  "name": "ShellCheck",
  "version": "0.1.3",
  "publisher": "Swellaby",
  "public": true,
  "tags": [
    "shell",
    "shellcheck"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "ShellCheck Extension",
  "categories": [
    "Azure Pipelines"
  ],
  "scopes": [
    "vso.extension",
    "vso.profile"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": ".publish"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "gallery-flags": [
    "Preview",
    "Public"
  ],
  "branding": {
    "color": "rgb(44, 62, 80)",
    "theme": "dark"
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/swellaby/azdo-shellcheck"
    },
    "support": {
      "uri": "https://github.com/swellaby/azdo-shellcheck/issues/new/choose"
    },
    "license": {
      "uri": "https://github.com/swellaby/azdo-shellcheck/blob/master/LICENSE"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/swellaby/azdo-shellcheck"
  },
  "contributions": [
    {
      "id": "shellcheck",
      "description": "Pipeline task to run ShellCheck",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": ".publish/tasks/shellcheck"
      }
    }
  ]
}
